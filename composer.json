{
  "name": "tm/yii2-service-services",
  "description": "Yii2 Microservice of Carts",
  "keywords": [
    "yii2",
    "service-services"
  ],
  "authors": [
    {
      "name": "Iryna Mychkova",
      "email": "imychkova@gmail.com",
      "role": "Developer"
    }
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "repositories":[
    {
      "type":"git",
      "url":"https://github.com/M0nsterLabs/yii2-migration-aware-module"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mice-tm/yii2-queue"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-intl": "*",
    "yiisoft/yii2": "^2.0.12",
    "yiisoft/yii2-swiftmailer": "^2.1.0",
    "yiisoft/yii2-authclient": "^2.1.4",
    "trntv/cheatsheet": "^0.1@dev",
    "trntv/yii2-command-bus": "^3.0.0",
    "vlucas/phpdotenv": "^2.4",
    "symfony/process": "^3.3",
    "templatemonster/yii2-migration-aware-module" : "^1.0.1",
    "indigerd/yii2-oauth2-auth-filter" : "^1.0.7",
    "indigerd/yii2-rest": "^1.0.7",
    "psr/log": "^1.0",
    "stomp-php/stomp-php": "^4.3",
    "bazilio/yii2-newrelic": "~0.0.7",
    "yiisoft/yii2-mongodb": "^2.1",
    "templatemonster/healthchecks" : "dev-master@dev",
    "yiisoft/yii2-queue": "^2.0",
    "yii2tech/embedded": "^1.0",
    "mice-tm/yii2-queue": "^0.0.2"
  },
  "require-dev": {
    "yiisoft/yii2-bootstrap": "^2.0.7",
    "yiisoft/yii2-debug": "^2.0.12",
    "yiisoft/yii2-jui": "^2.0.7",
    "yiisoft/yii2-gii": "^2.0.5",
    "yiisoft/yii2-faker": "^2.0.3",
    "codeception/codeception": "^2.3"
  },
  "suggest": {
    "trntv/yii2-debug-xhprof": "dev-master@dev"
  },
  "config": {
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    },
    "process-timeout": 1800
  },
  "extra": {
  }
}
